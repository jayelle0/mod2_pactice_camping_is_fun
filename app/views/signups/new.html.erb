<% if flash[:my_errors]%>
<% flash[:my_errors].each do |error|%> 
<%= error %> <br>
<% end %>
<% end %>

<%= form_for @signup do |f|%>
Camper: <%= f.collection_select :camper_id, Camper.all, :id, :name%> <br>
Activity: <%= f.collection_select :activity_id, Activity.all, :id, :name%> <br>
<%= f.label "Time"%>
<%= f.number_field :time %> <br>
<%= f.submit%>
<% end %>